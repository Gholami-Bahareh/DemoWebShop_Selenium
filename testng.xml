<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="DemoWebShopSuite">

 
  <test name="SmokeTests">
    <groups>
      <run>
        <include name="smoke"/>
      </run>
    </groups>
    <packages>
      <package name="tests"/>
    </packages>
  </test>
  

    <test name="RegressionTests">
    <groups>
      <run>
        <include name="regression"/>
      </run>
    </groups>
    <packages>
      <package name="tests"/>
    </packages>
  </test>


</suite>
